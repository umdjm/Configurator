#draggable {
    width: 150px;
    height: 150px;
    padding: 0.5em;
}
#demo {
    border:solid;
    clear:both;
    margin-top:150px;
}
#container {
    position:absolute;
    top:0px;
    left:0px;
    z-index:-1;
}

.container-row{
    height: 40px;
    margin-bottom:15px;
}
.container-box {
    border:solid;
    background-color:red;
    opacity:0;
    width: 30px;
    height: 30px;
    float:left;
}
.ui-state-active
{
    opacity:.4;
}

#outer-shell {
    margin-top:48px;
    margin-left:18px;
    width:700px;
    height: 100px;
}
#left-container {
    float:left;
}
#mid-container {
    margin-left:33px;
    float:left;
}
#mid-container .container-box {
    width:33px;
}
#right-container {
    margin-left:45px;
    float:left;
}